root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
indent_style = space
indent_size = 2
trim_trailing_whitespace = false

[*.cs]
indent_style = space
indent_size = 4

[*.xaml]
indent_style = space
indent_size = 4

[*.csproj]
indent_style = space
indent_size = 2

[*.sln]
indent_style = tab
indent_size = 4

[*.json]
indent_style = space
indent_size = 2
